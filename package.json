{
  "name": "Airspace",
  "version": "0.0.1",
  "description": "LeapMotion desktop client",
  "main": "index.html",
  "node-main": "node-main.js",
  "window": {
    "show": false,
    "toolbar": true
  },
  "dependencies": {
    "adm-zip": "0.4.3",
    "async": "0.2.8",
    "backbone": "1.0.0",
    "fs-extra": "0.6.1",
    "jade": "0.30.0",
    "plist": "git://github.com/proxv/node-plist.git#ea8aca44489a71c08d21e24b33efcbb5b149b52e",
    "stylus": "0.32.1",
    "underscore": "1.4.4"
  },
  "devDependencies": {
    "mocha": "1.9.x",
    "rewire": "1.1.3"
  },
  "scripts": {
    "test": "find ./test -name \"test*.js\" | xargs node_modules/mocha/bin/mocha",
    "unit-test": "node_modules/mocha/bin/mocha --recursive test/unit",
    "integration-test": "node_modules/mocha/bin/mocha --recursive test/integration"
  },
  "engine": "node >= 0.10.0"
